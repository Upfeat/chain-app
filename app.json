{
  "expo": {
    "name": "The Chain App",
    "slug": "chain-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/upfeat.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/upfeat.png",
      "resizeMode": "contain",
      "backgroundColor": "#d4484a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "Please allow chain to always use your location information, we won't upload or share your exact location to anywhere.",
        "UIBackgroundModes": [
          "location"
        ]
      }
    }
  }
}
